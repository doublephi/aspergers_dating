<div id="left_column">
  <h4 id="content_title_bar"> Sign Up</h4>
  <div id="new_user">
    <table>
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
      <tr>
        <td colspan="2"><%= devise_error_messages! %></td>
      </tr>
      <tr>
        <td><%= f.label :username %></td>
        <td><%= f.text_field :username %></td>
      </tr>
      <tr>
        <td><%= f.label :email %></td>
        <td><%= f.email_field :email %></td>
      </tr>
      <tr>
        <td><%= f.label :zipcode %></td>
        <td><%= f.text_field :zipcode %></td>
      </tr>
      <tr>
        <td><%= f.label :password %></td>
        <td><%= f.password_field :password %></td>
      </tr>
      <tr>
        <td><%= f.label :password_confirmation %></td>
        <td><%= f.password_field :password_confirmation %></td>
      </tr>
      <tr>
        <td></td>
        <td><%= f.submit "Sign up" %></td>
      </tr>
    <% end %>
    </table>
  </div>
</div>
<div id="right_column">
  <h4 id="content_title_bar">Advertisement</h4>
  <div id="tall_banner_container">
    <div id="tall_banner">
    </div>
  </div>
</div>

